#include <stdio.h>
int main()
{
  int i,sum=0,d1=0,d2=0;
  char p;
  while((p = getchar()) != '\n')
  {
    if (p >= 'a' && p <= 'z')
    {
      sum=sum+p-'a'+1;
    }
    else if(p >= 'A'&& p <= 'Z')
    {
      sum=sum+p-'A'+1;
    }
  }
  while(sum>0)
  {
    if(sum%2==0)
    {
       d2++;
    }
    else { d1++; }
    sum=sum/2;
  }
  printf ("%d%d",d2,d1);
  return 0;
}
